#: ../authconfig.py:120
#, python-format
msgid "usage: %s [options]"
msgstr "用法：%s [选项]"

#: ../authconfig.py:126
msgid "show this help message and exit"
msgstr "显示本帮助并退出"

#: ../authconfig.py:129
msgid "enable shadowed passwords by default"
msgstr "默认启用 shadow 密码"

#: ../authconfig.py:131
msgid "disable shadowed passwords by default"
msgstr "恢复在上一次配置更改前备份的配置文件"

#: ../authconfig.py:133
msgid "enable MD5 passwords by default"
msgstr "默认启用 MD5 密码"

#: ../authconfig.py:135
msgid "disable MD5 passwords by default"
msgstr "默认禁用 MD5 密码"

#: ../authconfig.py:138
msgid "hash/crypt algorithm for new passwords"
msgstr "用于新密码的散列/加密算法"

#: ../authconfig.py:141
msgid "enable NIS for user information by default"
msgstr "默认启用 NIS 用于用户信息"

#: ../authconfig.py:143
msgid "disable NIS for user information by default"
msgstr "默认禁用 NIS 用于用户信息"

#: ../authconfig.py:144
msgid "<domain>"
msgstr "<域>"

#: ../authconfig.py:145
msgid "default NIS domain"
msgstr "默认 NIS 域"

