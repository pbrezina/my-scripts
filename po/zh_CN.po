msgstr "默认 NIS 服务器"

#: ../authconfig.py:150
msgid "enable LDAP for user information by default"
msgstr "默认启用 LDAP 用于用户信息"

#: ../authconfig.py:152
msgid "disable LDAP for user information by default"
msgstr "默认禁用 LDAP 用于用户信息"

#: ../authconfig.py:154
msgid "enable LDAP for authentication by default"
msgstr "默认启用 LDAP 用于认证"

#: ../authconfig.py:156
msgid "disable LDAP for authentication by default"
msgstr "默认禁用 LDAP 用于认证"

#: ../authconfig.py:158
msgid "default LDAP server hostname or URI"
msgstr "默认 LDAP 服务器的主机名或 URI"

#: ../authconfig.py:159
msgid "<dn>"
msgstr "<dn>"

