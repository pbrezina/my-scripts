# ../authconfig.c:601
#: ../authconfig.c:626
#, c-format
msgid ""
"Usage: %s [options]\n"
"\n"
"     --useshadow\n"
"     --enableshadow             enable shadow passwords by default\n"
"     --disableshadow            disable shadow passwords by default\n"
"     --enablemd5                enable MD5 passwords by default\n"
"     --disablemd5               disable MD5 passwords by default\n"
"\n"
"     --enablenis                enable NIS\n"
"     --disablenis               disable NIS\n"
"     --nisdomain <domain>       default NIS domain\n"
"     --nisserver <server>       default NIS server\n"
"\n"
"     --enableldap               enable ldap for user information by default\n"
"     --disableldap              disable ldap for user information by "
"default\n"
"     --enableldapauth           enable ldap for authentication by default\n"
"     --disableldapauth          disable ldap for authentication by default\n"
"     --ldapserver <server>      default LDAP server\n"
"     --ldapbasedn <dn>          default LDAP base DN\n"
"\n"
"     --enablekrb5               enable kerberos authentication by default\n"
"     --disablekrb5              disable kerberos authentication by default\n"
"     --krb5kdc <server>         default kerberos KDC\n"
"     --krb5adminserver <server> default kerberos admin server\n"
"     --krb5realm <realm>        default kerberos realm\n"
"\n"
"     --enablehesiod             enable hesiod for user information by "
"default\n"
"     --disablehesiod            disable hesiod for user information by "
"default\n"
"     --hesiodlhs <lhs>          default hesiod LHS\n"
"     --hesiodrhs <rhs>          default hesiod RHS\n"
"\n"
"     --nostart                  do not start/stop ypbind\n"
"     --kickstart                don't display user interface\n"
"     --help                     show this screen\n"
msgstr ""
"Verwendung: %s [Optionen]\n"
"\n"
"     --useshadow\n"
"     --enableshadow             Shadow-Passwörter standardmäßig aktivieren\n"
"     --disableshadow            Shadow-Passwörter standardmäßig "
"deaktivieren\n"
"     --enablemd5                MD5-Passwörter standardmäßig aktivieren\n"
"     --disablemd5               MD5-Passwörter standardmäßig deaktivieren\n"
"\n"
"     --enablenis                NIS aktivieren\n"
"     --disablenis               NIS deaktivieren\n"
"     --nisdomain <Domäne>       standardmäßige NIS-Domäne\n"
"     --nisserver <Server>       standardmäßiger NIS-Server\n"
"\n"
"     --enableldap               ldap standardmäßig für Benutzerinformationen "
"verwenden\n"
"     --disableldap              ldap standardmäßig für Benutzerinformationen "
"deaktivieren\n"
"     --enableldapauth           ldap standardmäßig für Sicherheitsprüfung "
"verwenden\n"
"     --disableldapauth          ldap standardmäßig für Sicherheitsprüfung "
"deaktivieren\n"
"     --ldapserver <server>      standardmäßiger LDAP-Server\n"
"     --ldapbasedn <dn>          standardmäßiger LDAP-Basis-DN\n"
"\n"
"     --enablekrb5               Kerberos-Sicherheitsprüfung standardmäßig "
"aktivieren\n"
"     --disablekrb5              Kerberos-Sicherheitsprüfung standardmäßig "
"deaktivieren\n"
"     --krb5kdc <Server>         Standard-Kerberos-KDC\n"
"     --krb5adminserver <Server> standardmäßiger Kerberos-Admin-Server\n"
"     --krb5realm <Bereich>      standardmäßiger Kerberos-Bereich\n"
"\n"
"     --enablehesiod             Hesiod standardmäßig für "
"Benutzerinformationen aktivieren\n"
"     --disablehesiod            Hesiod standardmäßig für "
"Benutzerinformationen deaktivieren\n"
"     --hesiodlhs <lhs>          Standard-Hesiod-LHS\n"
"     --hesiodrhs <rhs>          Standard-Hesiod-RHS\n"
"\n"
"     --nostart                  ypbind nicht starten/anhalten\n"
"     --kickstart                keine Benutzeroberfläche anzeigen\n"
"     --help                     diesen Bildschirm anzeigen\n"

