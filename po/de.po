#: ../authconfig.c:691
#, fuzzy, c-format
msgid ""
"Usage: %s [options]\n"
"\n"
"     --nostart            do not start/stop yp\n"
"     --enablenis          enable nis by default\n"
"     --nisdomain <domain> default NIS domain\n"
"     --nisserver <server> default NIS server\n"
"     --useshadow          use shadow passwords\n"
"     --enablemd5          enable MD5 passwords\n"
"     --kickstart          don't display user interface\n"
"     --help               show this screen\n"
msgstr ""
"Verwendung: %s [Optionen]\n"
"\n"
"     --nostart            YP nicht starten/abbrechen\n"
"     --enablenis          NIS standardmäßig aktivieren\n"
"     --nisdomain <Domäne> NIS-Standarddomäne\n"
"     --nisserver <Server> NIS-Standardserver\n"
"     --useshadow          Shadow-Paßwörter verwenden\n"
"     --enablemd5          MD5-Paßwörter aktivieren\n"
"     --kickstart          Benutzerschnittstelle nicht anzeigen\n"
"     --help               Diese Hilfe anzeigen\n"

