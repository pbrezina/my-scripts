#: ../authconfig.c:620
#, c-format
msgid ""
"Usage: %s [options]\n"
"\n"
"     --useshadow\n"
"     --enableshadow             enable shadow passwords by default\n"
"     --disableshadow            disable shadow passwords by default\n"
"     --enablemd5                enable MD5 passwords by default\n"
"     --disablemd5               disable MD5 passwords by default\n"
"\n"
"     --enablenis                enable NIS\n"
"     --disablenis               disable NIS\n"
"     --nisdomain <domain>       default NIS domain\n"
"     --nisserver <server>       default NIS server\n"
"\n"
"     --enableldap               enable ldap for user information by default\n"
"     --disableldap              disable ldap for user information by "
"default\n"
"     --enableldapauth           enable ldap for authentication by default\n"
"     --disableldapauth          disable ldap for authentication by default\n"
"     --ldapserver <server>      default LDAP server\n"
"     --ldapbasedn <dn>          default LDAP base DN\n"
"\n"
"     --enablekrb5               enable kerberos authentication by default\n"
"     --disablekrb5              disable kerberos authentication by default\n"
"     --krb5kdc <server>         default kerberos KDC\n"
"     --krb5adminserver <server> default kerberos admin server\n"
"     --krb5realm <realm>        default kerberos realm\n"
"\n"
"     --enablehesiod             enable hesiod for user information by "
"default\n"
"     --disablehesiod            disable hesiod for user information by "
"default\n"
"     --hesiodlhs <lhs>          default hesiod LHS\n"
"     --hesiodrhs <rhs>          default hesiod RHS\n"
"\n"
"     --nostart                  do not start/stop ypbind\n"
"     --kickstart                don't display user interface\n"
"     --help                     show this screen\n"
msgstr ""
"Pou¾ití: %s [volby]\n"
"\n"
"     --useshadow\n"
"     --enableshadow             povolit standardnì stínová hesla\n"
"     --disableshadow            zakázat standardnì stínová hesla\n"
"     --enablemd5                povolit standardnì MD5 hesla\n"
"     --disablemd5               zakázat standardnì MD5 hesla\n"
"\n"
"     --enablenis                povolit NIS\n"
"     --disablenis               zakázat NIS\n"
"     --nisdomain <domain>       implicitní NIS doména\n"
"     --nisserver <server>       implicitní NIS server\n"
"\n"
"     --enableldap               povolit standardnì LDAP pro info o "
"u¾ivateli\n"
"     --disableldap              zakázat standardnì LDAP pro info o "
"u¾ivateli\n"
"     --enableldapauth           povolit standardnì LDAP pro autentizaci\n"
"     --disableldapauth          zakázat standardnì LDAP pro autentizaci\n"
"     --ldapserver <server>      implicitní LDAP server\n"
"     --ldapbasedn <dn>          implicitní LDAP base DN\n"
"\n"
"     --enablekrb5               povolit standardnì autentizaci kerberosem\n"
"     --disablekrb5              zakázat standardnì autentizaci kerberosem\n"
"     --krb5kdc <server>         implicitní KDC kerberosu\n"
"     --krb5adminserver <server> implicitní admin server kerberosu\n"
"     --krb5realm <realm>        implicitní realm kerberosu\n"
"\n"
"     --enablehesiod             povolit standardnì hesiod pro info o "
"u¾ivateli\n"
"     --disablehesiod            zakázat standardnì hesiod pro info o "
"u¾ivateli\n"
"     --hesiodlhs <lhs>          implicitní hesiod LHS\n"
"     --hesiodrhs <rhs>          implicitní hesiod RHS\n"
"\n"
"     --nostart                  nezastavovat/nestartovat ypbind\n"
"     --kickstart                nezobrazovat u¾ivatelské rozhraní\n"
"     --help                     tato obrazovka\n"

