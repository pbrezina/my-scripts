#: ../authconfig.c:620
#, c-format
msgid ""
"Usage: %s [options]\n"
"\n"
"     --useshadow\n"
"     --enableshadow             enable shadow passwords by default\n"
"     --disableshadow            disable shadow passwords by default\n"
"     --enablemd5                enable MD5 passwords by default\n"
"     --disablemd5               disable MD5 passwords by default\n"
"\n"
"     --enablenis                enable NIS\n"
"     --disablenis               disable NIS\n"
"     --nisdomain <domain>       default NIS domain\n"
"     --nisserver <server>       default NIS server\n"
"\n"
"     --enableldap               enable ldap for user information by default\n"
"     --disableldap              disable ldap for user information by "
"default\n"
"     --enableldapauth           enable ldap for authentication by default\n"
"     --disableldapauth          disable ldap for authentication by default\n"
"     --ldapserver <server>      default LDAP server\n"
"     --ldapbasedn <dn>          default LDAP base DN\n"
"\n"
"     --enablekrb5               enable kerberos authentication by default\n"
"     --disablekrb5              disable kerberos authentication by default\n"
"     --krb5kdc <server>         default kerberos KDC\n"
"     --krb5adminserver <server> default kerberos admin server\n"
"     --krb5realm <realm>        default kerberos realm\n"
"\n"
"     --enablehesiod             enable hesiod for user information by "
"default\n"
"     --disablehesiod            disable hesiod for user information by "
"default\n"
"     --hesiodlhs <lhs>          default hesiod LHS\n"
"     --hesiodrhs <rhs>          default hesiod RHS\n"
"\n"
"     --nostart                  do not start/stop ypbind\n"
"     --kickstart                don't display user interface\n"
"     --help                     show this screen\n"
msgstr ""
"Notkun: %s [rofar]\n"
"\n"
"     --useshadow\n"
"     --enableshadow             nota sjálfgefið falin lykilorð\n"
"     --disableshadow            ekki hafa falin lykilorð sjálfgefin\n"
"     --enablemd5                nota MD% lykilorð sjálfgefið\n"
"     --disablemd5               ekki nota MD% lykilorð sjálfgefið\n"
"\n"
"     --enablenis                nota NIS\n"
"     --disablenis               ekki nota NIS\n"
"     --nisdomain <lén>          sjálfgefið NIS lén\n"
"     --nisserver <þjónn>        sjálfgefinn NIS þjónn\n"
"\n"
"     --enableldap               nota sjálfgefið ldap fyrir "
"notendaupplýsingar\n"
"     --disableldap              ekki nota sjálfgefið ldap fyrir "
"notendaupplýsingar\n"
"     --enableldapauth           nota sjálfgefið ldap auðkenningu fyrir "
"notendur\n"
"     --disableldapauth          ekki nota sjálfgefið ldap auðkenningu fyrir "
"notendur\n"
"     --ldapserver <þjónn>       sjálfgefinn LDAP þjónn\n"
"     --ldapbasedn <dn>          sjálfgefið LDAP grunn DN\n"
"\n"
"     --enablekrb5               nota kerberos auðkenningu sjálfgefið\n"
"     --disablekrb5              ekki nota kerberos auðkenningu sjálfgefið\n"
"     --krb5kdc <server>         sjálfgefið kerberos KDC\n"
"     --krb5adminserver <þjónn>  sjálfgefinn kerberos stjórnþjónn\n"
"     --krb5realm <svæði>        sjálfgefið kerberos svæði\n"
"\n"
"     --enablehesiod             nota sjálfgefið hesiod fyrir "
"notendaupplýsingar\n"
"     --disablehesiod            ekki nota sjálfgefið hesiod fyrir "
"notendaupplýsingar\n"
"     --hesiodlhs <lhs>          sjálfgefið hesiod LHS\n"
"     --hesiodrhs <rhs>          sjálfgefið hesiod RHS\n"
"\n"
"     --nostart                  ekki ræsa eða stöðva ypbind\n"
"     --kickstart                ekki birta notendaflöt\n"
"     --help                     sýna þennan skjá\n"

