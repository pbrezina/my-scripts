#: ../authconfig.py:115
#, python-format
msgid "usage: %s [options]"
msgstr "használat: %s [opciók]"

#: ../authconfig.py:121
msgid "show this help message and exit"
msgstr "ezt a súgó üzenetet megjeleníti és kilép"

#: ../authconfig.py:124
msgid "enable shadowed passwords by default"
msgstr "árnyékjelszavak engedélyezése az alapértelmezésként"

#: ../authconfig.py:126
msgid "disable shadowed passwords by default"
msgstr "árnyékjelszavak tiltása az alapértelmezésként"

