#: ../authconfig.py:116
#, python-format
msgid "usage: %s [options]"
msgstr "használat: %s [opciók]"

#: ../authconfig.py:122
msgid "show this help message and exit"
msgstr "ezt a súgó üzenetet megjeleníti és kilép"

#: ../authconfig.py:125
msgid "enable shadowed passwords by default"
msgstr "árnyékjelszavak engedélyezése az alapértelmezésként"

#: ../authconfig.py:127
msgid "disable shadowed passwords by default"
msgstr "árnyékjelszavak tiltása az alapértelmezésként"

#: ../authconfig.py:129
msgid "enable MD5 passwords by default"
msgstr "MD5 jelszavak engedélyezése az alapértelmezett"

#: ../authconfig.py:131
msgid "disable MD5 passwords by default"
msgstr "MD5 jelszavak tiltása az alapértelmezett"

#: ../authconfig.py:134
msgid "hash/crypt algorithm for new passwords"
msgstr "szóró/titkosító algoritmus új jelszavakra"

#: ../authconfig.py:137
msgid "enable NIS for user information by default"
msgstr "a NIS felhasználói információkhoz való használatának engedélyezése alapértelmezésben"

#: ../authconfig.py:139
msgid "disable NIS for user information by default"
msgstr "a NIS felhasználói információkhoz való használatának tiltása alapértelmezésben"

#: ../authconfig.py:140 ../authconfig.py:254
msgid "<domain>"
msgstr "<tartomány>"

#: ../authconfig.py:141
msgid "default NIS domain"
msgstr "alapértelmezett NIS-tartomány"

#: ../authconfig.py:142 ../authconfig.py:153 ../authconfig.py:196
#: ../authconfig.py:198
msgid "<server>"
msgstr "<kiszolgáló>"

#: ../authconfig.py:143
msgid "default NIS server"
msgstr "alapértelmezett NIS-kiszolgáló"

#: ../authconfig.py:146
msgid "enable LDAP for user information by default"
msgstr "az LDAP felhasználói információkhoz való használatának engedélyezése alapértelmezetten"

#: ../authconfig.py:148
msgid "disable LDAP for user information by default"
msgstr "az LDAP felhasználói információkhoz való használatának tiltása alapértelmezetten"

#: ../authconfig.py:150
msgid "enable LDAP for authentication by default"
msgstr "az LDAP engedélyezése azonosításhoz alapértelmezésben"

#: ../authconfig.py:152
msgid "disable LDAP for authentication by default"
msgstr "az LDAP azonosításhoz való használatának tiltása alapértelmezésben"

#: ../authconfig.py:154
msgid "default LDAP server hostname or URI"
msgstr "alapértelmezett LDAP kiszolgáló neve vagy URI-ja"

#: ../authconfig.py:155
msgid "<dn>"
msgstr "<dn>"

#: ../authconfig.py:156
msgid "default LDAP base DN"
msgstr "alapértelmezett LDAP alap-DN-je"

#: ../authconfig.py:158
msgid "enable use of TLS with LDAP (RFC-2830)"
msgstr "TLS (RFC-2830) engedélyezése LDAP használatakor"

#: ../authconfig.py:160
msgid "disable use of TLS with LDAP (RFC-2830)"
msgstr "TLS (RFC-2830) tiltása LDAP használatakor"

#: ../authconfig.py:162
msgid "enable use of RFC-2307bis schema for LDAP user information lookups"
msgstr "RFC-2307bis séma engedélyezése LDAP felhasználói információk lekéréséhez"

#: ../authconfig.py:164
msgid "disable use of RFC-2307bis schema for LDAP user information lookups"
msgstr "RFC-2307bis séma tiltása LDAP felhasználói információk lekéréséhez"

#: ../authconfig.py:165
msgid "<URL>"
msgstr "<URL>"

#: ../authconfig.py:166
msgid "load CA certificate from the URL"
msgstr "töltse le a CA tanúsítványát az URL-ről"

#: ../authconfig.py:169
msgid "enable authentication with smart card by default"
msgstr "csipkártya-alapú azonosítás engedélyezése alapértelmezésben"

#: ../authconfig.py:171
msgid "disable authentication with smart card by default"
msgstr "csipkártya-alapú azonosítás letiltása alapértelmezésben"

#: ../authconfig.py:173
msgid "require smart card for authentication by default"
msgstr "kötelező csipkártya-alapú azonosítás alapértelmezésben"

#: ../authconfig.py:175
msgid "do not require smart card for authentication by default"
msgstr "nem kötelező a csipkártya-alapú azonosítás alapértelmezésben"

#: ../authconfig.py:176
msgid "<module>"
msgstr "<modul>"

#: ../authconfig.py:177
msgid "default smart card module to use"
msgstr "alapértelmezett csipkártya-modul"

#: ../authconfig.py:180
msgid "action to be taken on smart card removal"
msgstr "művelet a csipkártya eltávolításakor"

#: ../authconfig.py:183
msgid "enable authentication with fingerprint readers by default"
msgstr "ujjlenyomat-olvasóval történő azonosítás engedélyezése alapértelmezésben"

#: ../authconfig.py:185
msgid "disable authentication with fingerprint readers by default"
msgstr "ujjlenyomat-olvasóval történő azonosítás letiltása alapértelmezésben"

#: ../authconfig.py:188
msgid "enable automatic per-user ecryptfs"
msgstr "automatikus felhasználónkénti ecryptfs engedélyezése"

#: ../authconfig.py:190
msgid "disable automatic per-user ecryptfs"
msgstr "automatikus felhasználónkénti ecryptfs kikapcsolása"

#: ../authconfig.py:193
msgid "enable kerberos authentication by default"
msgstr "Kerberos-azonosítás engedélyezése alapértelmezésben"

#: ../authconfig.py:195
msgid "disable kerberos authentication by default"
msgstr "Kerberos-azonosítás tiltása alapértelmezésben"

#: ../authconfig.py:197
msgid "default kerberos KDC"
msgstr "alapértelmezett Kerberos-KDC"

#: ../authconfig.py:199
msgid "default kerberos admin server"
msgstr "alapértelmezett Kerberos admin-kiszolgáló"

#: ../authconfig.py:200 ../authconfig.py:221 ../authconfig.py:256
msgid "<realm>"
msgstr "<tartomány>"

#: ../authconfig.py:201
msgid "default kerberos realm"
msgstr "alapértelmezett Kerberos-tartomány"

#: ../authconfig.py:203
msgid "enable use of DNS to find kerberos KDCs"
msgstr "DNS használatának engedélyezése Kerberos KDC keresésére"

#: ../authconfig.py:205
msgid "disable use of DNS to find kerberos KDCs"
msgstr "DNS használatának tiltása Kerberos KDC keresésére"

#: ../authconfig.py:207
msgid "enable use of DNS to find kerberos realms"
msgstr "DNS használatának engedélyezése Kerberos-tartományok keresésére"

#: ../authconfig.py:209
msgid "disable use of DNS to find kerberos realms"
msgstr "DNS használatának tiltása Kerberos-tartományok keresésére"

#: ../authconfig.py:212
msgid "enable winbind for user information by default"
msgstr "a Winbind engedélyezése alapértelmezésben felhasználói adatok lekérdezéséhez"

#: ../authconfig.py:214
msgid "disable winbind for user information by default"
msgstr "a Winbind letiltása alapértelmezésben felhasználói adatok lekérdezéséhez"

#: ../authconfig.py:216
msgid "enable winbind for authentication by default"
msgstr "a Winbind engedélyezése alapértelmezésben felhasználóazonosításhoz"

#: ../authconfig.py:218
msgid "disable winbind for authentication by default"
msgstr "a Winbind tiltása alapértelmezésben felhasználó azonosításhoz"

#: ../authconfig.py:220
msgid "security mode to use for samba and winbind"
msgstr "biztonsági mód a Samba és a Winbind használatához"

#: ../authconfig.py:222
msgid "default realm for samba and winbind when security=ads"
msgstr "alapértelmezett Kerberos-tartomány a Sambához és a Winbindhoz security=ads esetén"

#: ../authconfig.py:223 ../authconfig.py:258
msgid "<servers>"
msgstr "<kiszolgálók>"

#: ../authconfig.py:224
msgid "names of servers to authenticate against"
msgstr "az azonosítást végző kiszolgálók nevei"

#: ../authconfig.py:225
msgid "<workgroup>"
msgstr "<munkacsoport>"

#: ../authconfig.py:226
msgid "workgroup authentication servers are in"
msgstr "munkacsoport-azonosítási kiszolgálók a következőben:"

#: ../authconfig.py:227 ../authconfig.py:229
msgid "<lowest-highest>"
msgstr "<legalacsonyabb-legmagasabb>"

#: ../authconfig.py:228
msgid "uid range winbind will assign to domain or ads users"
msgstr "a Winbind által a tartomány- illetve ADS-felhasználóknak biztosított UID-tartomány"

#: ../authconfig.py:230
msgid "gid range winbind will assign to domain or ads users"
msgstr "a Winbind által a tartomány- illetve ADS-felhasználóknak biztosított GID-tartomány"

#: ../authconfig.py:232
msgid ""
"the character which will be used to separate the domain and user part of "
"winbind-created user names if winbindusedefaultdomain is not enabled"
msgstr "a Winbind által létrehozott felhasználói nevekben a tartomány és a felhasználó elválasztására használt karakter (a winbindusedefaultdomain kikapcsolt állapota esetén)"

#: ../authconfig.py:234
msgid ""
"the directory which winbind-created users will have as home directories"
msgstr "a mappa, amelyet a Winbind által létrehozott felhasználók saját mappaként kapnak"

#: ../authconfig.py:236
msgid "the group which winbind-created users will have as their primary group"
msgstr "a Winbind által létrehozott felhasználók elsődleges csoportja"

#: ../authconfig.py:238
msgid "the shell which winbind-created users will have as their login shell"
msgstr "a shell, amelyet a Winbind által létrehozott felhasználók kapnak bejelentkezési parancsértelmezőként"

#: ../authconfig.py:240
msgid ""
"configures winbind to assume that users with no domain in their user names "
"are domain users"
msgstr "úgy állítja be a Winbindot, hogy azok a felhasználók, akiknek a felhasználóneve nem tartalmaz tartományt, tartományi felhasználókként legyenek kezelve"

#: ../authconfig.py:242
msgid ""
"configures winbind to assume that users with no domain in their user names "
"are not domain users"
msgstr "úgy állítja be a Winbindot, hogy azok a felhasználók, akiknek a felhasználóneve nem tartalmaz tartományt, nem tartományi felhasználókként legyenek kezelve"

#: ../authconfig.py:244
msgid "configures winbind to allow offline login"
msgstr "winbind beállítása kapcsolat nélküli bejelentkezés engedélyezésére"

#: ../authconfig.py:246
msgid "configures winbind to prevent offline login"
msgstr "winbind beállítása kapcsolat nélküli bejelentkezés megakadályozására"

#: ../authconfig.py:248
msgid "join the winbind domain or ads realm now as this administrator"
msgstr "csatlakozás a Winbind-tartományhoz vagy ADS-tartományhoz rendszergazdaként most"

#: ../authconfig.py:251
msgid "enable IPAv2 for user information and authentication by default"
msgstr "engedélyezze az IPAv2-t felhasználói információknál és azonosításnál alapértelmezésben"

#: ../authconfig.py:253
msgid "disable IPAv2 for user information and authentication by default"
msgstr "kapcsolja ki az IPAv2-t felhasználói információknál és azonosításnál alapértelmezésben"

#: ../authconfig.py:255
msgid "the IPAv2 domain the system should be part of"
msgstr "IPAv2 tartomány a rendszer részének kell lennie"

#: ../authconfig.py:257
msgid "the realm for the IPAv2 domain"
msgstr "IPAv2 tartomány terjedelme"

#: ../authconfig.py:259
msgid "the server for the IPAv2 domain"
msgstr "IPAv2 tartomány kiszolgálója"

#: ../authconfig.py:261
msgid "do not setup the NTP against the IPAv2 domain"
msgstr "ne állítsa be az NTP-t tiltásra IPAv2 tartománynál"

#: ../authconfig.py:263
msgid "setup the NTP against the IPAv2 domain (default)"
msgstr "állítsa be az NTP-t tiltásra IPAv2 tartománynál (alapértelmezés)"

#: ../authconfig.py:265
msgid "join the IPAv2 domain as this account"
msgstr "csatlakozzon az IPAv2 tartományhoz mint ezzel a felhasználóval"

#: ../authconfig.py:268
msgid "enable wins for hostname resolution"
msgstr "a Wins gépnév-feloldáshoz való használatának engedélyezése"

#: ../authconfig.py:270
msgid "disable wins for hostname resolution"
msgstr "a Wins gépnév-feloldáshoz való használatának tiltása"

#: ../authconfig.py:273
msgid "prefer dns over wins or nis for hostname resolution"
msgstr "DNS előnyben részesítése a WINS vagy NIS gépnév-feloldással szemben"

#: ../authconfig.py:275
msgid "do not prefer dns over wins or nis for hostname resolution"
msgstr "ne részesítse előnyben a DNS-t a WINS és NIS gépnévfeloldással szemben"

#: ../authconfig.py:278
msgid "enable hesiod for user information by default"
msgstr "a felhasználói információk Hesioddal való kezelésének engedélyezése alapértelmezésben"

#: ../authconfig.py:280
msgid "disable hesiod for user information by default"
msgstr "a felhasználói információk Hesioddal való kezelésének tiltása alapértelmezésben"

#: ../authconfig.py:282
msgid "default hesiod LHS"
msgstr "alapértelmezett Hesiod LHS"

#: ../authconfig.py:284
msgid "default hesiod RHS"
msgstr "alapértelmezett Hesiod RHS"

#: ../authconfig.py:287
msgid ""
"enable SSSD for user information by default with manually managed "
"configuration"
msgstr "SSSD engedélyezése felhasználói adatokhoz alapértelmezés szerint kézzel kezelt konfigurációval"

#: ../authconfig.py:289
msgid ""
"disable SSSD for user information by default (still used for supported "
"configurations)"
msgstr "SSSD letiltása felhasználói adatokhoz alapértelmezés szerint (még ma is használják támogatott konfigurációk)"

#: ../authconfig.py:291
msgid ""
"enable SSSD for authentication by default with manually managed "
"configuration"
msgstr "SSSD engedélyezése a hitelesítésekhez alapból manuálisan kezelt konfigurációval"

#: ../authconfig.py:293
msgid ""
"disable SSSD for authentication by default (still used for supported "
"configurations)"
msgstr "SSSD letiltása a hitelesítésekhez az alapértelmezés szerint (még ma is használják támogatott konfigurációknál)"

#: ../authconfig.py:295
msgid "never use SSSD implicitly even for supported configurations"
msgstr "soha ne használja az SSSD-t, közvetve se a támogatott konfigurációkhoz"

#: ../authconfig.py:297
msgid "use SSSD implicitly if it supports the configuration"
msgstr "használjon SSSD-t, ha az támogatja a konfigurációt"

#: ../authconfig.py:300
msgid "enable caching of user credentials in SSSD by default"
msgstr "felhasználói információk gyorstárazásának engedélyezése alapértelmezésben"

#: ../authconfig.py:302
msgid "disable caching of user credentials in SSSD by default"
msgstr "felhasználói információk gyorstárazásának kikapcsolása alapértelmezésben"

#: ../authconfig.py:305
msgid ""
"enable caching of user information by default (automatically disabled when "
"SSSD is used)"
msgstr "felhasználói információk gyorstárazásának engedélyezése (SSSD használatakor automatikusan letiltásra kerül)"

#: ../authconfig.py:307
msgid "disable caching of user information by default"
msgstr "felhasználói információk gyorstárazásának kikapcsolása alapértelmezésben"

#: ../authconfig.py:310
msgid "local authorization is sufficient for local users"
msgstr "helyi felhasználók számára elegendő a helyi jogosultság ellenőrzése"

#: ../authconfig.py:312
msgid "authorize local users also through remote service"
msgstr "a helyi felhasználók jogosultságait is távoli szolgáltatással ellenőrizze"

#: ../authconfig.py:315
msgid "check access.conf during account authorization"
msgstr "access.conf _ellenőrzése felhasználó hitelesítésekor"

#: ../authconfig.py:317
msgid "do not check access.conf during account authorization"
msgstr "_ne ellenőrizze az access.conf-ot felhasználó hitelesítéskor"

#: ../authconfig.py:320
msgid "authenticate system accounts by network services"
msgstr "rendszer felhasználóit hálózati szolgáltatásokkal azonosítsa"

#: ../authconfig.py:322
msgid "authenticate system accounts by local files only"
msgstr "rendszer felhasználóit csak helyi fájlokkal azonosítsa"

#: ../authconfig.py:325
msgid "create home directories for users on their first login"
msgstr "saját mappa létrehozása az első bejelentkezéskor"

#: ../authconfig.py:327
msgid "do not create home directories for users on their first login"
msgstr "ne hozzon létre saját mappát az első bejelentkezéskor"

#: ../authconfig.py:329 ../authconfig.py:331 ../authconfig.py:333
#: ../authconfig.py:335
msgid "<number>"
msgstr "<szám>"

#: ../authconfig.py:330
msgid "minimum length of a password"
msgstr "jelszó minimális hossza"

#: ../authconfig.py:332
msgid "minimum number of character classes in a password"
msgstr "karakterosztályok minimális mennyisége a jelszóban"

#: ../authconfig.py:334
msgid "maximum number of same consecutive characters in a password"
msgstr "egymást követő karakterek maximális mennyisége a jelszóban"

#: ../authconfig.py:336
msgid "maximum number of consecutive characters of same class in a password"
msgstr "egymást követő, azonos osztályú karakterek maximális mennyiség a jelszóban"

#: ../authconfig.py:338
msgid "require at least one lowercase character in a password"
msgstr "legalább egy kisbetű szükséges a jelszóban"

#: ../authconfig.py:340
msgid "do not require lowercase characters in a password"
msgstr "nincs szükség kisbetűkre a jelszóban"

#: ../authconfig.py:342
msgid "require at least one uppercase character in a password"
msgstr "legalább egy nagybetű szükséges a jelszóban"

#: ../authconfig.py:344
msgid "do not require uppercase characters in a password"
msgstr "nincs szükség nagybetűkre a jelszóban"

#: ../authconfig.py:346
msgid "require at least one digit in a password"
msgstr "legalább egy szám szükséges a jelszóban"

#: ../authconfig.py:348
msgid "do not require digits in a password"
msgstr "nincs szükség számokra a jelszóban"

#: ../authconfig.py:350
msgid "require at least one other character in a password"
msgstr "legalább egy másik típusú karakter szükséges a jelszóban"

#: ../authconfig.py:352
msgid "do not require other characters in a password"
msgstr "nem szükségesek másik karakterek a jelszóban"

#: ../authconfig.py:355
msgid "do not start/stop portmap, ypbind, and nscd"
msgstr "a portmap, az ypbind és az nscd ne legyen elindítva/leállítva"

#: ../authconfig.py:358
msgid "do not update the configuration files, only print new settings"
msgstr "ne frissítse a konfigurációs fájlokat, csak írja ki az új beállításokat"

#: ../authconfig.py:362
msgid "display Back instead of Cancel in the main dialog of the TUI"
msgstr "A TUI fő párbeszédablakában a Vissza jelenjen meg a Mégse helyett"

#: ../authconfig.py:364
msgid "do not display the deprecated text user interface"
msgstr "ne jelenítse meg az elavult szöveges felhasználói felületet"

#: ../authconfig.py:367
msgid "opposite of --test, update configuration files with changed settings"
msgstr "a --test ellentéteként, a konfigurációs fájlok frissítése a megváltozott beállításokkal"

#: ../authconfig.py:370
msgid "update all configuration files"
msgstr "minden konfigurációs fájl frissítése"

#: ../authconfig.py:373
msgid "probe network for defaults and print them"
msgstr "hálózat felderítése az alapértelmezett beállításokért, majd azok kiírása"

#: ../authconfig.py:375 ../authconfig.py:378
msgid "<name>"
msgstr "<név>"

#: ../authconfig.py:376
msgid "save a backup of all configuration files"
msgstr "biztonsági mentés létrehozása minden konfigurációs fájlról"

#: ../authconfig.py:379
msgid "restore the backup of configuration files"
msgstr "konfigurációs fájlok visszaállítása a biztonsági mentésekből"

#: ../authconfig.py:382
msgid ""
"restore the backup of configuration files saved before the previous "
"configuration change"
msgstr "konfigurációs fájlok visszaállítása az előző beállítás módosítása előtti állapotra"

#: ../authconfig.py:387
msgid "unexpected argument"
msgstr "váratlan argumentum"

#: ../authconfig.py:518
msgid "The passminlen option value is not an integer"
msgstr "A passminlen opció értéke nem egész"

#: ../authconfig.py:523
msgid "The passminclass option value is not an integer"
msgstr "A passminclass opció értéke nem egész"

#: ../authconfig.py:528
msgid "The passmaxrepeat option value is not an integer"
msgstr "A passmaxrepeat  opció értéke nem egész"

#: ../authconfig.py:533
msgid "The passmaxclassrepeat option value is not an integer"
msgstr "A passmaxclassrepeat opció értéke nem egész"

#: ../authconfig.py:554
msgid "Bad smart card removal action specified."
msgstr "Hibás a megadott csipkártya-eltávolítási művelet."

#: ../authconfig.py:563
msgid "Unknown password hashing algorithm specified, using sha256."
msgstr "Ismeretlen jelszó szóró algoritmust adtak meg, SHA256 alkalmazása..."

#: ../authconfig.py:598
msgid "can only be run as root"
msgstr "csak rendszergazdaként futtatható"

#: ../authconfig.py:614
msgid "dialog was cancelled"
msgstr "a párbeszéd meg lett szakítva"

#: ../authconfig.py:646 ../authconfig-gtk.py:523
#, python-format
msgid ""
"The %s file was not found, but it is required for %s support to work properly.\n"
"Install the %s package, which provides this file."
msgstr "A(z) %s fájl nem található, de szükség van rá a(z) %s-támogatás működéséhez.\nTelepítse a(z) %s csomagot, amely tartalmazza ezt a fájlt."

#: ../authconfig.py:648 ../authconfig.py:994
msgid "Warning"
msgstr "Figyelem"

#: ../authconfig.py:648 ../authconfig.py:865 ../authconfig.py:873
#: ../authconfig.py:879 ../authconfig.py:888 ../authconfig.py:896
#: ../authconfig.py:940 ../authconfig.py:994
msgid "Ok"
msgstr "OK"

#: ../authconfig.py:652
msgid "caching"
