#: ../authconfig.c:642
#, fuzzy, c-format
msgid ""
"Usage: %s [options]\n"
"\n"
"     --useshadow\n"
"     --enableshadow             enable shadow passwords by default\n"
"     --disableshadow            disable shadow passwords by default\n"
"     --enablemd5                enable MD5 passwords by default\n"
"     --disablemd5               disable MD5 passwords by default\n"
"\n"
"     --enablenis                enable NIS\n"
"     --disablenis               disable NIS\n"
"     --nisdomain <domain>       default NIS domain\n"
"     --nisserver <server>       default NIS server\n"
"\n"
"     --enableldap               enable ldap for user information by default\n"
"     --disableldap              disable ldap for user information by "
"default\n"
"     --enableldaptls            enable use of TLS with ldap\n"
"     --disableldaptls           disable use of TLS with ldap\n"
"     --enableldapauth           enable ldap for authentication by default\n"
"     --disableldapauth          disable ldap for authentication by default\n"
"     --ldapserver <server>      default LDAP server\n"
"     --ldapbasedn <dn>          default LDAP base DN\n"
"\n"
"     --enablekrb5               enable kerberos authentication by default\n"
"     --disablekrb5              disable kerberos authentication by default\n"
"     --krb5kdc <server>         default kerberos KDC\n"
"     --krb5adminserver <server> default kerberos admin server\n"
"     --krb5realm <realm>        default kerberos realm\n"
"\n"
"     --enablehesiod             enable hesiod for user information by "
"default\n"
"     --disablehesiod            disable hesiod for user information by "
"default\n"
"     --hesiodlhs <lhs>          default hesiod LHS\n"
"     --hesiodrhs <rhs>          default hesiod RHS\n"
"\n"
"     --nostart                  do not start/stop ypbind\n"
"     --kickstart                don't display user interface\n"
"     --help                     show this screen\n"
msgstr ""
"Использование: %s [options]\n"
"\n"
"     --useshadow\n"
"     --enableshadow             разрешить скрытые пароли по умолчанию\n"
"     --disableshadow            запретить скрытые пароли по умолчанию\n"
"     --enablemd5                разрешить пароли MD5 по умолчанию\n"
"     --disablemd5               запретить пароли MD5 по умолчанию\n"
"\n"
"     --enablenis                разрешить NIS\n"
"     --disablenis               запретить NIS\n"
"     --nisdomain <domain>       домен NIS по умолчанию\n"
"     --nisserver <server>       сервер NIS по умолчанию\n"
"\n"
"     --enableldap               разрешить ldap для информации о\n"
"                                  пользователе по умолчанию\n"
"     --disableldap              запретить ldap для информации о\n"
"                                  пользователе по умолчанию\n"
"     --enableldapauth           разрешить ldap для аутентификации\n"
"                                  по умолчанию\n"
"     --disableldapauth          запретить ldap для аутентификации\n"
"                                  по умолчанию\n"
"     --ldapserver <server>      сервер LDAP по умолчанию\n"
"     --ldapbasedn <dn>          LDAP base DN по умолчанию\n"
"\n"
"     --enablekrb5               разрешить аутентификацию kerberos по "
"умолчанию\n"
"     --disablekrb5              запретить аутентификацию kerberos по "
"умолчанию\n"
"     --krb5kdc <server>         kerberos KDC по умолчанию\n"
"     --krb5adminserver <server> административный сервер kerberos по "
"умолчанию\n"
"     --krb5realm <realm>        kerberos realm по умолчанию\n"
"\n"
"     --enablehesiod             разрешить ldap для информации о\n"
"                                  пользователе по умолчанию\n"
"     --disablehesiod            запретить ldap для информации о\n"
"                                  пользователе по умолчанию\n"
"     --hesiodlhs <lhs>          hesiod LHS по умолчанию\n"
"     --hesiodrhs <rhs>          hesiod RHS по умолчанию\n"
"\n"
"     --nostart                  не перезапускать ypbind\n"
"     --kickstart                не показывать интерфейс пользователя\n"
"     --help                     показать этот экран\n"

