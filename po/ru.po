msgstr "<количество>"

#: ../authconfig.py:303
msgid "minimum length of a password"
msgstr "Минимальная длина пароля"

#: ../authconfig.py:305
msgid "minimum number of character classes in a password"
msgstr "минимальное количество символов в пароле"

#: ../authconfig.py:307
