#: ../authconfig.c:1257
#, c-format
msgid ""
"Usage: %s [options]\n"
"\n"
"     --nostart            do not start/stop yp\n"
"     --enablenis          enable nis by default\n"
"     --nisdomain <domain> default NIS domain\n"
"     --nisserver <server> default NIS server\n"
"     --useshadow          use shadow passwords\n"
"     --enablemd5          enable MD5 passwords\n"
"     --kickstart          don't display user interface\n"
"     --help               show this screen\n"
msgstr ""
"Применение: %s [опции]\n"
"\n"
"     --nostart            не останавливать/запускать yp\n"
"     --enablenis          разрешить nis по умолчанию\n"
"     --nisdomain <domain> домен NIS по умолчанию\n"
"     --nisserver <server> сервер NIS по умолчанию\n"
"     --useshadow          использовать скрытые пароли\n"
"     --enablemd5          использовать пароли MD5\n"
"     --kickstart          не отображать интерфейс пользователя\n"
"     --help               показать этот экран\n"

