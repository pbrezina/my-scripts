#: ../authconfig.py:116
#, python-format
msgid "usage: %s [options]"
msgstr "Формат: %s [параметры]"

#: ../authconfig.py:122
msgid "show this help message and exit"
msgstr "показать эту справку и выйти"

#: ../authconfig.py:125
msgid "enable shadowed passwords by default"
msgstr "включить скрытые пароли (по умолчанию)"

#: ../authconfig.py:127
msgid "disable shadowed passwords by default"
msgstr "выключить скрытые пароли (по умолчанию)"

#: ../authconfig.py:129
msgid "enable MD5 passwords by default"
msgstr "включить пароли MD5 (по умолчанию)"

#: ../authconfig.py:131
msgid "disable MD5 passwords by default"
msgstr "выключить пароли MD5 (по умолчанию)"

#: ../authconfig.py:134
msgid "hash/crypt algorithm for new passwords"
msgstr "алгоритм хэширования/шифрования для новых паролей"

#: ../authconfig.py:137
msgid "enable NIS for user information by default"
msgstr "использовать NIS для обработки информации пользователей"

#: ../authconfig.py:139
msgid "disable NIS for user information by default"
msgstr "не использовать NIS для обработки информации пользователей"

#: ../authconfig.py:140 ../authconfig.py:256
