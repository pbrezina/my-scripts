#: ../authconfig.py:120
#, python-format
msgid "usage: %s [options]"
msgstr "utilizzo: %s [opzioni]"

#: ../authconfig.py:126
msgid "show this help message and exit"
msgstr "mostra questo messaggio di aiuto ed esce"

#: ../authconfig.py:129
