#: ../authconfig.c:693
#, c-format
msgid ""
"Usage: %s [options]\n"
"\n"
"     --nostart            do not start/stop yp\n"
"     --enablenis          enable nis by default\n"
"     --nisdomain <domain> default NIS domain\n"
"     --nisserver <server> default NIS server\n"
"     --useshadow          use shadow passwords\n"
"     --enablemd5          enable MD5 passwords\n"
"     --kickstart          don't display user interface\n"
"     --help               show this screen\n"
msgstr ""
"Usage: %s [options]\n"
"\n"
"     --nostart            non start/stop yp\n"
"     --enablenis          abilita nis di default\n"
"     --nisdomain <domain> abilita dominio NIS\n"
"     --nisserver <server> server NIS di default\n"
"     --useshadow          usa shadow password\n"
"     --enablemd5          abilita MD5 password\n"
"     --kickstart          non mostrare l'interfaccia utente\n"
"     --help               mostra questo schermo\n"

