msgstr "aktivera autentisering med fingeravtrycksläsare som standard"

#: ../authconfig.py:189
msgid "disable authentication with fingerprint readers by default"
msgstr "inaktivera autentisering med fingeravtrycksläsare som standard"

#: ../authconfig.py:192
msgid "enable automatic per-user ecryptfs"
msgstr "aktivera automatiskt ecryptfs per användare"

#: ../authconfig.py:194
msgid "disable automatic per-user ecryptfs"
msgstr "inaktivera automatiskt ecryptfs per användare"

#: ../authconfig.py:197
msgid "enable kerberos authentication by default"
msgstr "aktivera kerberos-autentisering som standard"

#: ../authconfig.py:199
msgid "disable kerberos authentication by default"
msgstr "inaktivera kerberos-autentisering som standard"

#: ../authconfig.py:201
msgid "default kerberos KDC"
msgstr "standard-KDC för kerberos"

#: ../authconfig.py:203
