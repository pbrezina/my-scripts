#: ../authconfig.c:626
#, c-format
msgid ""
"Usage: %s [options]\n"
"\n"
"     --useshadow\n"
"     --enableshadow             enable shadow passwords by default\n"
"     --disableshadow            disable shadow passwords by default\n"
"     --enablemd5                enable MD5 passwords by default\n"
"     --disablemd5               disable MD5 passwords by default\n"
"\n"
"     --enablenis                enable NIS\n"
"     --disablenis               disable NIS\n"
"     --nisdomain <domain>       default NIS domain\n"
"     --nisserver <server>       default NIS server\n"
"\n"
"     --enableldap               enable ldap for user information by default\n"
"     --disableldap              disable ldap for user information by "
"default\n"
"     --enableldapauth           enable ldap for authentication by default\n"
"     --disableldapauth          disable ldap for authentication by default\n"
"     --ldapserver <server>      default LDAP server\n"
"     --ldapbasedn <dn>          default LDAP base DN\n"
"\n"
"     --enablekrb5               enable kerberos authentication by default\n"
"     --disablekrb5              disable kerberos authentication by default\n"
"     --krb5kdc <server>         default kerberos KDC\n"
"     --krb5adminserver <server> default kerberos admin server\n"
"     --krb5realm <realm>        default kerberos realm\n"
"\n"
"     --enablehesiod             enable hesiod for user information by "
"default\n"
"     --disablehesiod            disable hesiod for user information by "
"default\n"
"     --hesiodlhs <lhs>          default hesiod LHS\n"
"     --hesiodrhs <rhs>          default hesiod RHS\n"
"\n"
"     --nostart                  do not start/stop ypbind\n"
"     --kickstart                don't display user interface\n"
"     --help                     show this screen\n"
msgstr ""
"Usage: %s [options]\n"
"\n"
"     --useshadow\n"
"     --enableshadow             activare parole shadow implicit\n"
"     --disableshadow            dezactivare parole shadow implicit\n"
"     --enablemd5                activare parole MD5 implicit\n"
"     --disablemd5               dezactivare parole MD5 implicit\n"
"\n"
"     --enablenis                activeazã NIS\n"
"     --disablenis               dezactiveazã NIS\n"
"     --nisdomain <domain>       domeniu NIS implicit\n"
"     --nisserver <server>       server NIS implict\n"
"\n"
"     --enableldap               activare LDAP implict pentru cîmpuri GECOS\n"
"     --disableldap              dezactivare LDAP implicit pentru cîmpuri "
"GECOS\n"
"     --enableldapauth           activare autentificare LDAP implicit\n"
"     --disableldapauth          dezactivare autentificare LDAP implicit\n"
"     --ldapserver <server>      server LDAP implicit\n"
"     --ldapbasedn <dn>          Base DN pentru LDAP implicit\n"
"\n"
"     --enablekrb5               activare autentificare Kerberos 5 implict\n"
"     --disablekrb5              dezactivare autentificare Kerberos 5 "
"implicit\n"
"     --krb5kdc <server>         KDC implicit\n"
"     --krb5adminserver <server> kerberos admin server\n"
"     --krb5realm <realm>        realm kerberos implicit\n"
"\n"
"     --enablehesiod             activare hesiod pentru cîmpuri GECOS "
"implict\n"
"     --disablehesiod            dezactivare hesiod pentru cîmpuri GECOS "
"implict\n"
"     --hesiodlhs <lhs>          Hesiod LHS implicit\n"
"     --hesiodrhs <rhs>          Hesiod RHS implicit\n"
"\n"
"     --nostart                  fãrã pornire/oprire a serviciului  ypbind\n"
"     --kickstart                nu afiºa interfeþa utilizator\n"
"     --help                     acest text pentru modul de utilizare\n"

