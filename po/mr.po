#: ../authconfig.py:79
#, python-format
msgid "usage: %s [options]"
msgstr "वापर: %s [पर्याय]"

#: ../authconfig.py:86
msgid "enable shadowed passwords by default"
msgstr "छायांकित गुप्तशब्द मुलभूतरित्या कार्यान्वित करा"

#: ../authconfig.py:88
msgid "disable shadowed passwords by default"
msgstr "छायांकित गुप्तशब्द मुलभूतरित्या अकार्यान्वीत करा"

#: ../authconfig.py:90
msgid "enable MD5 passwords by default"
msgstr "MD5 गुप्तशब्द मुलभूतरित्या कार्यान्वित करा"

#: ../authconfig.py:92
msgid "disable MD5 passwords by default"
msgstr "MD5 गुप्तशब्द मुलभूतरित्या अकार्यान्वीत करा"

#: ../authconfig.py:95
msgid "hash/crypt algorithm for new passwords"
msgstr "नविन गुप्तशब्द करीता hash/crypt अल्गोरिदम"

#: ../authconfig.py:98
msgid "enable NIS for user information by default"
msgstr "उपयोगकर्त्याच्या माहितीसाठी NIS मुलभूतरित्या कार्यान्वित करा"

#: ../authconfig.py:100
msgid "disable NIS for user information by default"
msgstr "उपयोगकर्त्याच्या माहितीसाठी NIS मुलभूतरित्या अकार्यान्वीत करा"

#: ../authconfig.py:101
msgid "<domain>"
msgstr "<domain>"

#: ../authconfig.py:102
