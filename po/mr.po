msgstr "मुलभूत NIS डोमेन"

#: ../authconfig.py:103 ../authconfig.py:114 ../authconfig.py:148
#: ../authconfig.py:150
msgid "<server>"
msgstr "<server>"

#: ../authconfig.py:104
msgid "default NIS server"
