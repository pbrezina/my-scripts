#: ../authconfig.py:52
#, python-format
msgid "usage: %s [options]"
msgstr "பயன்பாடு: %s [விருப்பங்கள்]"

#: ../authconfig.py:59
msgid "enable shadowed passwords by default"
msgstr "நிழற் கடவுச்சொல்லை இயல்பாக நிலையில் செயல்படுத்துகிறது"

#: ../authconfig.py:61
msgid "disable shadowed passwords by default"
msgstr "நிழற் கடவுச்சொல்லை இயல்பான நிலையில் செயல்படுத்தாதே"

#: ../authconfig.py:63
msgid "enable MD5 passwords by default"
msgstr "MD5 கடவுச்சொற்களை இயல்பான நிலையில் செயல்படுத்து"

#: ../authconfig.py:65
msgid "disable MD5 passwords by default"
msgstr "முன்னிருப்பாக MD5 கடவுச்சொற்களை செயலிழக்கச் செய்"

#: ../authconfig.py:68
msgid "enable NIS for user information by default"
msgstr "NIS ஐ முன்னிருப்பாக பயனர் தகவலுக்காக செயல்படுத்தவும்"

#: ../authconfig.py:70
msgid "disable NIS for user information by default"
msgstr "NIS ஐ முன்னிருப்பாக பயனர் தகவலுக்காக செயலிழக்கச்செய்"

#: ../authconfig.py:71
msgid "<domain>"
msgstr "<domain>"

#: ../authconfig.py:72
msgid "default NIS domain"
msgstr "இயல்பான NIS களம்"

#: ../authconfig.py:73 ../authconfig.py:84 ../authconfig.py:122
#: ../authconfig.py:124
msgid "<server>"
msgstr "<server>"

#: ../authconfig.py:74
msgid "default NIS server"
msgstr "முன்னிருப்பு NIS சேவகன்"

#: ../authconfig.py:77
msgid "enable LDAP for user information by default"
msgstr "பயன்படுத்துபவர் தகவலுக்காக LDAPஐ இயல்பாக செயல்படுத்து"

#: ../authconfig.py:79
msgid "disable LDAP for user information by default"
msgstr "பயன்படுத்துபவர் தகவலுக்காக LDAPஐ இயல்பாக செயலிழக்கச் செய்"

#: ../authconfig.py:81
msgid "enable LDAP for authentication by default"
msgstr "LDAPஐ சரிபார்த்தலுக்காக இயல்பாக செயல்படுத்து"

#: ../authconfig.py:83
msgid "disable LDAP for authentication by default"
msgstr "LDAPஐ சரிபார்த்தலுக்காக இயல்பாக செயலிழக்கச் செய்"

#: ../authconfig.py:85
msgid "default LDAP server"
msgstr "இயல்பானனுக்கும் LDAP சேவகன்"

#: ../authconfig.py:86
msgid "<dn>"
msgstr "<dn>"

#: ../authconfig.py:87
msgid "default LDAP base DN"
msgstr "முன்னிருப்பு LDAP அடிப்படை DN"

#: ../authconfig.py:89
msgid "enable use of TLS with LDAP"
msgstr "TLS ன் பயனை LDAP உடன் செயல்படுத்து"

#: ../authconfig.py:91
msgid "disable use of TLS with LDAP"
msgstr "TLS ன் பயனை LDAP உடன் செயலிழக்கச் செய்"

#: ../authconfig.py:92
msgid "<URL>"
msgstr "<URL>"

#: ../authconfig.py:93
msgid "load CA certificate from the URL"
msgstr "CA சான்றிதழை URLலிலிருந்து ஏற்றவும்"

#: ../authconfig.py:96
msgid "enable authentication with smart card by default"
msgstr "முன்னிருப்பாக ஸ்மார்ட்கார்ட்டினால் அங்கீகாரத்தை செயல்படுத்தவும்"

#: ../authconfig.py:98
msgid "disable authentication with smart card by default"
msgstr "முன்னிருப்பாக ஸ்மார்ட்கார்ட்டின் அங்கீகாரத்தை செயல்நீக்கவும்"

#: ../authconfig.py:100
msgid "require smart card for authentication by default"
msgstr "முன்னிருப்பாக அங்கீகாரத்திற்கு ஸ்மார்ட்கார்ட் தேவைப்படுகிறது"

#: ../authconfig.py:102
msgid "do not require smart card for authentication by default"
msgstr "முன்னிருப்பாக அங்கீகாரத்திற்கு ஸ்மார்ட்கார்ட் தேவையில்லை"

#: ../authconfig.py:103
msgid "<module>"
msgstr "<module>"

#: ../authconfig.py:104
msgid "default smart card module to use"
msgstr "பயன்படுத்த முன்னிருப்பு ஸ்மார்ட்கார்ட் தொகுதி"

#: ../authconfig.py:116
msgid "action to be taken on smart card removal"
msgstr "ஸ்மார்ட்கார்ட்டை நீக்க எடுக்கப்பட்ட நடவடிக்கை"

#: ../authconfig.py:119
msgid "enable kerberos authentication by default"
msgstr "kerberos சரிபார்த்தலை முன்னிருப்பாக செயல்படுத்தவும்"

#: ../authconfig.py:121
msgid "disable kerberos authentication by default"
msgstr "kerberos சரிபார்த்தலை முன்னிருப்பாக செயலிழக்கச் செய்யவும்"

#: ../authconfig.py:123
msgid "default kerberos KDC"
msgstr "முன்னிருப்பு kerberos KDC"

#: ../authconfig.py:125
msgid "default kerberos admin server"
msgstr "முன்னிருப்பு kerberos நிர்வாக சேவையகம்"

#: ../authconfig.py:126 ../authconfig.py:156
msgid "<realm>"
msgstr "<realm>"

#: ../authconfig.py:127
msgid "default kerberos realm"
msgstr "முன்னிருப்பு kerberos realm"

#: ../authconfig.py:129
msgid "enable use of DNS to find kerberos KDCs"
msgstr "கர்ப்ரோஸ் KDC க்களை கண்டுபிடிக்க DNS ஐ செயல்படுத்து"

#: ../authconfig.py:131
msgid "disable use of DNS to find kerberos KDCs"
msgstr "கர்ப்ரோஸ் KDC க்களை கண்டுபிடிக்க DNS ஐ செயல்நீக்கு"

#: ../authconfig.py:133
msgid "enable use of DNS to find kerberos realms"
msgstr "கர்ப்ரோஸ் வட்டாரம் கண்டுபிடிக்க DNS ஐ செயல்படுத்து"

#: ../authconfig.py:135
msgid "disable use of DNS to find kerberos realms"
msgstr "கர்ப்ரோஸ் வட்டாரங்களை கண்டுபிடிக்க DNS ஐ செயலிழக்க செய்"

#: ../authconfig.py:138
msgid "enable SMB authentication by default"
msgstr "SMB சரிபார்த்தலை முன் இருக்குமாறு செயல்படுத்து"

#: ../authconfig.py:140
msgid "disable SMB authentication by default"
msgstr "SMB சரிபார்த்தலை இயல்பாக செயலிழக்கச் செய்"

#: ../authconfig.py:141
msgid "<servers>"
msgstr "<servers>"

#: ../authconfig.py:142
msgid "names of servers to authenticate against"
msgstr "சரிபார்த்தலுக்காக இருக்கும் கணினிகளின் பெயர்கள்"

#: ../authconfig.py:143
msgid "<workgroup>"
msgstr "<workgroup>"

#: ../authconfig.py:144
msgid "workgroup authentication servers are in"
msgstr "பணிக்குழு சரிபார்க்கும் கணினிகள் இருக்கும் இடம்"

#: ../authconfig.py:147
msgid "enable winbind for user information by default"
msgstr "பொதுவான பயனர் தகவலுக்கு winbind ஐ செயல்படுத்து"

#: ../authconfig.py:149
msgid "disable winbind for user information by default"
msgstr "பொதுவான பயனர் தகவலுக்கு winbind ஐ செயல்நீக்கு"

#: ../authconfig.py:151
msgid "enable winbind for authentication by default"
msgstr "முன்னிருப்பாக அனுமதிக்கு winbind ஐ செயல்படுத்து"

#: ../authconfig.py:153
msgid "disable winbind for authentication by default"
msgstr "முன்னிருப்பாக அனுமதிக்கு winbind ஐ செயலிழக்க செய்"

#: ../authconfig.py:155
msgid "security mode to use for samba and winbind"
msgstr "சம்பா மற்றும் வின்பைண்டிற்கு பயன்படும் பாதுகாப்பு வகை"

#: ../authconfig.py:157
msgid "default realm for samba and winbind when security=ads"
msgstr "பொதுவான வட்டாரத்திற்கான சம்பா மற்றும் winbind பாதுகாப்பு=ads"

#: ../authconfig.py:158 ../authconfig.py:160
msgid "<lowest-highest>"
msgstr "<lowest-highest>"

#: ../authconfig.py:159
msgid "uid range winbind will assign to domain or ads users"
msgstr "uid வரம்புகளை வின்பைண்ட் ads பயனாளர்களுக்கும் ஒதுக்குகிறது"

#: ../authconfig.py:161
msgid "gid range winbind will assign to domain or ads users"
msgstr "gid வின்பைண்ட் வரம்பு களத்திற்கான ads பயனர்களுக்கு ஒதுக்குகிறது"

#: ../authconfig.py:163
msgid ""
"the character which will be used to separate the domain and user part of "
"winbind-created user names if winbindusedefaultdomain is not enabled"
msgstr ""
"winbindusedefaultdomain செயலில் இல்லாத போது வின் பைண்ட் உருவாக்கிய பயனர் பகுதியையும் "
"டொமைனையும் பிரிக்க பயன்படும் எழுத்து."

#: ../authconfig.py:165
msgid "the directory which winbind-created users will have as home directories"
msgstr "வின் பைண்ட் உருவாக்கிய அடைவை பயன்படுத்துபவர்கள் தங்கள் இல்ல அடைவுகளாகக் கொள்வர்"

#: ../authconfig.py:167
msgid "the group which winbind-created users will have as their primary group"
msgstr "வின் பைன்ட் உருவாக்கிய குழுப்பெட்டியை பயனாளர்கள் முதல் குழுப்பெட்டியாகக் கொள்வர்"

#: ../authconfig.py:169
msgid "the shell which winbind-created users will have as their login shell"
msgstr "வின்பைன்ட்-உருவாக்கிய ஷெல் பயனர்கள் உள்நுழை ஷெல்லாக எடுத்துக்கொள்வர்"

#: ../authconfig.py:171
msgid ""
"configures winbind to assume that users with no domain in their user names "
"are domain users"
msgstr ""
"தங்கள் பெயர்களில் களம் இல்லாத பயனாளர்களை களத்தின் பயனாளர்களாக நினைத்துக் கொள்ள வின் பைண்டை "
"வடிவமைக்கிறது"

#: ../authconfig.py:173
msgid ""
"configures winbind to assume that users with no domain in their user names "
"are not domain users"
msgstr ""
"தங்கள் பெயர்களில் களம் இல்லாத பயனாளர்களை களத்தின் பயனாளர்களாக நினைத்துக் கொள்ளாமல் வின் "
"பைண்டை வடிவமைக்கிறது"

#: ../authconfig.py:175
msgid "join the winbind domain or ads realm now as this administrator"
msgstr "இந்த நிர்வாகியாக வின் பைண்ட் களத்தில் அல்லது ads வட்டாரத்தில் சேரவும்"

#: ../authconfig.py:178
msgid "enable wins for hostname resolution"
msgstr "கணிணித் தெளிவுத்திறனுக்கான வின்ஸை செயல்படுத்துத்தவும்"

#: ../authconfig.py:180
msgid "disable wins for hostname resolution"
msgstr "கணினித் தெளிவுத்திறனுக்கான வின்ஸை செயலிழக்கச் செய்"

#: ../authconfig.py:183
msgid "enable hesiod for user information by default"
msgstr "பயன்படுத்துபவர் தகவலுக்கான hesiodஐ இயல்பாக செயல்படுத்து"

#: ../authconfig.py:185
msgid "disable hesiod for user information by default"
msgstr "பயன்படுத்துபவர் தகவலுக்கான hesiodஐ இயல்பாக செயலிழக்கச் செய்"

#: ../authconfig.py:187
msgid "default hesiod LHS"
msgstr "அடிப்படை hesiod LHS"

#: ../authconfig.py:189
msgid "default hesiod RHS"
msgstr "முன்னிருப்பாக hesiod RHS"

#: ../authconfig.py:192
msgid "enable caching of user information by default"
msgstr "பயன்படுத்துபவர் தகவலை தற்காலிக சேமிப்பில் இயல்பாக செயல்படுத்து"

#: ../authconfig.py:194
msgid "disable caching of user information by default"
msgstr "முன்னிருப்பாக பயனர் தகவலை தற்காலிக சேமிப்பில் செயலிழக்கச் செய்"

#: ../authconfig.py:197
msgid "local authorization is sufficient for local users"
msgstr "இருப்பிட பயனர்களுக்கு போதுமான இருப்பிட உரிமம்"

#: ../authconfig.py:199
msgid "authorize local users also through remote service"
msgstr "இருப்பிட பயனர்களை தூரத்து சேவைகள் மூலமும் அங்கீகரிக்கவும்"

#: ../authconfig.py:202
msgid "authenticate system accounts by network services"
msgstr "அமைப்பு கணக்குகளை பிணைய சேவைகளால் மட்டும் அங்கீகரிக்கவும்"

#: ../authconfig.py:204
msgid "authenticate system accounts by local files only"
msgstr "அமைப்பு கணக்குகளை இருப்பிட கோப்புகளால் மட்டும் அங்கீகரிக்கவும்"

#: ../authconfig.py:207
msgid "do not start/stop portmap, ypbind, and nscd"
msgstr "portmap, ypbind, and nscdஐ துவக்காதே/நிறுத்தாதே"

#: ../authconfig.py:210
msgid "do not update the configuration files, only print new settings"
msgstr "கட்டமைக்கப்பட்ட கோப்புகளை புதுப்பிக்க வேண்டாம், புதிய அமைவுகளை அச்சடித்தால் போதும்"

#: ../authconfig.py:214
msgid "display Back instead of Cancel in the main dialog of the TUI"
msgstr "TUI ன் முதன்மை உரையாடலை ரத்து செய்வதற்கு பதில் பின்னால் காட்டவும்"

#: ../authconfig.py:216
msgid "do not display the deprecated text user interface"
msgstr "பயனர் இடைமுக இணைப்பை காட்டாதே"

#: ../authconfig.py:219
msgid "opposite of --test, update configuration files with changed settings"
msgstr "மாற்றப்பட்ட அமைவுகளுடன் கட்டமைக்கப்பட்ட கோப்புகளை புதுப்பிக்கவும்"

#: ../authconfig.py:222
msgid "update all configuration files"
msgstr "அனைத்து கட்டமைப்பட்ட கோப்புகளையும் புதுப்பி"

#: ../authconfig.py:225
msgid "probe network for defaults and print them"
msgstr "இயல்பான அச்சியந்திரத்திற்கான வலைப்பின்னலை கண்டுபிடி"

#: ../authconfig.py:230
msgid "unexpected argument"
msgstr "எதிர்பாராத மதிப்பு"

#: ../authconfig.py:332
#, fuzzy
msgid "Bad smart card removal action specified."
msgstr "பயன்படுத்த முன்னிருப்பு ஸ்மார்ட்கார்ட் தொகுதி"

#: ../authconfig.py:361
msgid "can only be run as root"
msgstr "ரூட்டாக மட்டுமே இயக்க முடியும்"

#: ../authconfig.py:368
msgid "dialog was cancelled"
msgstr "உரையாடல் ரத்துச் செய்யப்பட்டது"

#: ../authconfig.py:391
#, python-format
msgid ""
"The %s file was not found, but it is required for %s support to work "
"properly.\n"
"Install the %s package, which provides this file."
msgstr ""
"%s கோப்பு காணவில்லை. ஆனால் %s ஆதரவு ஒழுங்காக வேலை செய்ய இது தேவை.\n"
" இந்த கோப்பினை தரும் %s பணிதொகுப்பை நிறுவுக."

#: ../authconfig.py:393 ../authconfig.py:748
