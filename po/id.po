#: ../authconfig.c:1266
#, c-format
msgid ""
"Usage: %s [options]\n"
"\n"
"     --nostart            do not start/stop yp\n"
"     --enablenis          enable nis by default\n"
"     --nisdomain <domain> default NIS domain\n"
"     --nisserver <server> default NIS server\n"
"     --useshadow          use shadow passwords\n"
"     --enablemd5          enable MD5 passwords\n"
"     --kickstart          don't display user interface\n"
"     --help               show this screen\n"
msgstr ""
"Cara Pakai: %s [options]\n"
"\n"
"     --nostart            jangan start/stop yp\n"
"     --enablenis          jalankan nis secara default\n"
"     --nisdomain <domain> default NIS domain\n"
"     --nisserver <server> default NIS server\n"
"     --useshadow          gunakan shadow password\n"
"     --enablemd5          gunakan MD5 password\n"
"     --kickstart          jangan pakai user interface\n"
"     --help               tampilkan layar help\n"

