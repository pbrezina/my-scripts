#: ../authconfig.c:642
#, fuzzy, c-format
msgid ""
"Usage: %s [options]\n"
"\n"
"     --useshadow\n"
"     --enableshadow             enable shadow passwords by default\n"
"     --disableshadow            disable shadow passwords by default\n"
"     --enablemd5                enable MD5 passwords by default\n"
"     --disablemd5               disable MD5 passwords by default\n"
"\n"
"     --enablenis                enable NIS\n"
"     --disablenis               disable NIS\n"
"     --nisdomain <domain>       default NIS domain\n"
"     --nisserver <server>       default NIS server\n"
"\n"
"     --enableldap               enable ldap for user information by default\n"
"     --disableldap              disable ldap for user information by "
"default\n"
"     --enableldaptls            enable use of TLS with ldap\n"
"     --disableldaptls           disable use of TLS with ldap\n"
"     --enableldapauth           enable ldap for authentication by default\n"
"     --disableldapauth          disable ldap for authentication by default\n"
"     --ldapserver <server>      default LDAP server\n"
"     --ldapbasedn <dn>          default LDAP base DN\n"
"\n"
"     --enablekrb5               enable kerberos authentication by default\n"
"     --disablekrb5              disable kerberos authentication by default\n"
"     --krb5kdc <server>         default kerberos KDC\n"
"     --krb5adminserver <server> default kerberos admin server\n"
"     --krb5realm <realm>        default kerberos realm\n"
"\n"
"     --enablehesiod             enable hesiod for user information by "
"default\n"
"     --disablehesiod            disable hesiod for user information by "
"default\n"
"     --hesiodlhs <lhs>          default hesiod LHS\n"
"     --hesiodrhs <rhs>          default hesiod RHS\n"
"\n"
"     --nostart                  do not start/stop ypbind\n"
"     --kickstart                don't display user interface\n"
"     --help                     show this screen\n"
msgstr ""
"Uso: %s [opcións]\n"
"\n"
"     --useshadow\n"
"     --enableshadow             activar contrasinais shadow por defecto\n"
"     --disableshadow            desactivar contrasinais shadow por defecto\n"
"     --enablemd5                activar contrasinais MD5 por defecto\n"
"     --disablemd5               desactivar contrasinais MD5 por defecto\n"
"\n"
"     --enablenis                activar NIS\n"
"     --disablenis               desactivar NIS\n"
"     --nisdomain <dominio>      dominio NIS por defecto\n"
"     --nisserver <servidor>     servidor NIS por defecto\n"
"\n"
"     --enableldap               activar ldap para información de usuario\n"
"                                por defecto\n"
"     --disableldap              desactivar ldap para información de usuario\n"
"                                por defecto\n"
"     --enableldapauth           activar ldap para autenticación por defecto\n"
"     --disableldapauth          desactivar ldap para autenticación por "
"defecto\n"
"     --ldapserver <servidor>    servidor LDAP por defecto\n"
"     --ldapbasedn <dn>          DN base de LDAP por defecto\n"
"\n"
"     --enablekrb5               activar autenticación kerberos por defecto\n"
"     --disablekrb5              desactivar autenticación kerberos por "
"defecto\n"
"     --krb5kdc <servidor>       KDC de kerberos por defecto\n"
"     --krb5adminserver <servidor> servidor de administración de kerberos "
"por\n"
"                                  defecto\n"
"     --krb5realm <realm>        realm de kerberos por defecto\n"
"\n"
"     --enablehesiod             activar hesiod para información de usuario\n"
"                                por defecto\n"
"     --disablehesiod            desactivar hesiod para información de "
"usuario\n"
"                                por defecto\n"
"     --hesiodlhs <lhs>          LHS de hesiod por defecto\n"
"     --hesiodrhs <rhs>          RHS de hesiod por defecto\n"
"\n"
"     --nostart                  non iniciar/parar ypbind\n"
"     --kickstart                non amosa-la interface de usuario\n"
"     --help                     amosar esta pantalla\n"

