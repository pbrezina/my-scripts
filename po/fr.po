#: ../authconfig.c:601
#, c-format
msgid ""
"Usage: %s [options]\n"
"\n"
"     --useshadow\n"
"     --enableshadow             enable shadow passwords by default\n"
"     --disableshadow            disable shadow passwords by default\n"
"     --enablemd5                enable MD5 passwords by default\n"
"     --disablemd5               disable MD5 passwords by default\n"
"\n"
"     --enablenis                enable NIS\n"
"     --disablenis               disable NIS\n"
"     --nisdomain <domain>       default NIS domain\n"
"     --nisserver <server>       default NIS server\n"
"\n"
"     --enableldap               enable ldap for user information by default\n"
"     --disableldap              disable ldap for user information by "
"default\n"
"     --enableldapauth           enable ldap for authentication by default\n"
"     --disableldapauth          disable ldap for authentication by default\n"
"     --ldapserver <server>      default LDAP server\n"
"     --ldapbasedn <dn>          default LDAP base DN\n"
"\n"
"     --enablekrb5               enable kerberos authentication by default\n"
"     --disablekrb5              disable kerberos authentication by default\n"
"     --krb5kdc <server>         default kerberos KDC\n"
"     --krb5adminserver <server> default kerberos admin server\n"
"     --krb5realm <realm>        default kerberos realm\n"
"\n"
"     --enablehesiod             enable hesiod for user information by "
"default\n"
"     --disablehesiod            disable hesiod for user information by "
"default\n"
"     --hesiodlhs <lhs>          default hesiod LHS\n"
"     --hesiodrhs <rhs>          default hesiod RHS\n"
"\n"
"     --nostart                  do not start/stop ypbind\n"
"     --kickstart                don't display user interface\n"
"     --help                     show this screen\n"
msgstr ""
"Utilisation : %s [options]\n"
"\n"
"     --useshadow\n"
"     --enableshadow             activer mots de passe masqués par défaut\n"
"     --disableshadow            désactiver mots de passe masqués par défaut\n"
"     --enablemd5                activer mots de passe MD5 par défaut\n"
"     --disablemd5               désactiver mots de passe MD5 par défaut\n"
"\n"
"     --enablenis                activer NIS\n"
"     --disablenis               désactiver NIS\n"
"     --nisdomain <domain>       domaine NIS par défaut\n"
"     --nisserver <server>       serveur NIS par défaut\n"
"\n"
"     --enableldap               activer ldap pour les informations\n"
"                                utilisateur par défaut\n"
"     --disableldap              désactiver ldap pour les informations\n"
"                                utilisateur par défaut\n"
"     --enableldapauth           activer ldap pour l'authentification\n"
"                                par défaut\n"
"     --disableldapauth          désactiver ldap pour l'authentification\n"
"                                par défaut\n"
"     --ldapserver <server>      serveur LDAP par défaut\n"
"     --ldapbasedn <dn>          DN de base LDAP par défaut "
"\n"
"     --enablekrb5               activer l'authentification kerberos par défaut\n"
"     --disablekrb5              désactiver l'authentification kerberos\n"
"                                par défaut\n"
"     --krb5kdc <server>         KDC kerberos par défaut\n"
"     --krb5adminserver <serveur> serveur admin kerberos par défaut\n"
"     --krb5realm <zone>         zone kerberos par défaut "
"\n"
"     --enablehesiod             activer hesiod pour les informations\n"
"                                utilisateur par défaut\n"
"     --disablehesiod            désactiver hesiod pour les informations\n"
"                                utilisateur par défaut\n"
"     --hesiodlhs <lhs>          LHS hesiod par défaut\n"
"     --hesiodrhs <rhs>          RHS hesiod par défaut "
"\n"
"     --nostart                  ne pas démarrer/arrêter ypbind\n"
"     --kickstart                ne pas afficher d'interface utilisateur\n"
"     --help                     afficher cet écran\n"

