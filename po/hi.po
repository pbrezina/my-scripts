#: ../authconfig.py:80
#, python-format
msgid "usage: %s [options]"
msgstr "प्रयोग: %s [विकल्प]"

#: ../authconfig.py:87
msgid "enable shadowed passwords by default"
msgstr "मूलभूत रूप से छायांकित शब्दकूटों को सक्रिय करें"

#: ../authconfig.py:89
msgid "disable shadowed passwords by default"
msgstr "मूलभूत रूप से छायांकित शब्दकूटों को निष्क्रिय करें"

#: ../authconfig.py:91
msgid "enable MD5 passwords by default"
msgstr "मूलभूत रूप से MD5 शब्दकूटों को सक्रिय करें"

#: ../authconfig.py:93
msgid "disable MD5 passwords by default"
msgstr "मूलभूत रूप से MD5 शब्दकूटों को निष्क्रिय करें"

#: ../authconfig.py:96
msgid "hash/crypt algorithm for new passwords"
msgstr "hash/crypt अल्गोरिथम नए शब्दकूट के लिए"

#: ../authconfig.py:99
msgid "enable NIS for user information by default"
msgstr "मूलभूत रूप से उपयोक्ता जानकारी के लिए NIS सक्रिय करें"

#: ../authconfig.py:101
msgid "disable NIS for user information by default"
msgstr "मूलभूत रूप से उपयोक्ता जानकारी के लिए NIS निष्क्रिय करें"

#: ../authconfig.py:102
msgid "<domain>"
msgstr "<domain>"

#: ../authconfig.py:103
msgid "default NIS domain"
msgstr "मूलभूत NIS डोमेन"

#: ../authconfig.py:104 ../authconfig.py:115 ../authconfig.py:149
#: ../authconfig.py:151
msgid "<server>"
msgstr "<server>"

#: ../authconfig.py:105
msgid "default NIS server"
msgstr "मूलभूत NIS सर्वर"

#: ../authconfig.py:108
msgid "enable LDAP for user information by default"
msgstr "मूलभूत रूप से उपयोक्ता जानकारी के लिए LDAP को सक्रिय करें"

#: ../authconfig.py:110
msgid "disable LDAP for user information by default"
msgstr "मूलभूत रूप से उपयोक्ता जानकारी के लिए LDAP को निष्क्रिय करें"

#: ../authconfig.py:112
msgid "enable LDAP for authentication by default"
msgstr "मूलभूत रूप से सत्यापन के लिए LDAP को सक्रिय करें"

#: ../authconfig.py:114
msgid "disable LDAP for authentication by default"
msgstr "मूलभूत रूप से सत्यापन के लिए LDAP को निष्क्रिय करें"

#: ../authconfig.py:116
msgid "default LDAP server hostname or URI"
msgstr "मूलभूत LDAP सर्वर होस्टनेम या URI"

#: ../authconfig.py:117
msgid "<dn>"
msgstr "<dn>"

#: ../authconfig.py:118
msgid "default LDAP base DN"
msgstr "मूलभूत LDAP बेस DN"

#: ../authconfig.py:120
msgid "enable use of TLS with LDAP (RFC-2830)"
msgstr "LDAP के साथ TLS का प्रयोग सक्रिय करें (RFC-2830)"

#: ../authconfig.py:122
msgid "disable use of TLS with LDAP (RFC-2830)"
msgstr "LDAP के साथ TLS का प्रयोग निष्क्रिय करें (RFC-2830)"

#: ../authconfig.py:123
msgid "<URL>"
msgstr "<URL>"

#: ../authconfig.py:124
msgid "load CA certificate from the URL"
msgstr "CA प्रमाणपत्र URL से लोड करें"

#: ../authconfig.py:127
msgid "enable authentication with smart card by default"
msgstr "स्मार्टकार्ड के साथ मूलभूल रूप से सत्यापन सक्रिय करें"

#: ../authconfig.py:129
msgid "disable authentication with smart card by default"
msgstr "स्मार्टकार्ड के साथ मूलभूल रूप से सत्यापन निष्क्रिय करें"

#: ../authconfig.py:131
msgid "require smart card for authentication by default"
msgstr "मूलभूत रूप से सत्यापन के लिए स्मार्टकार्ड जरूरी"

#: ../authconfig.py:133
msgid "do not require smart card for authentication by default"
msgstr "मूलभूत रूप से सत्यापन के लिए स्मार्टकार्ड जरूरी नहीं"

#: ../authconfig.py:134
msgid "<module>"
msgstr "<module>"

#: ../authconfig.py:135
msgid "default smart card module to use"
msgstr "प्रयोग के लिये मूलभूत स्मार्टकार्ड मॉड्यूल"

#: ../authconfig.py:138
msgid "action to be taken on smart card removal"
msgstr "स्मार्टकार्ड प्रतिस्थापन पर की गई कार्डवाई"

#: ../authconfig.py:141
msgid "enable authentication with fingerprint readers by default"
msgstr "तयशुदा रूप से फिंगरप्रिंट पाठक से सत्यापन सक्रिय करें"

#: ../authconfig.py:143
msgid "disable authentication with fingerprint readers by default"
msgstr "तयशुदा रूप से फिंगरप्रिंट पाठक से सत्यापन निष्क्रिय करें"

#: ../authconfig.py:146
msgid "enable kerberos authentication by default"
msgstr "मूलभूत रूप से करबरोस सत्यापन को सक्रिय करें"

#: ../authconfig.py:148
msgid "disable kerberos authentication by default"
msgstr "मूलभूत रूप से करबरोस सत्यापन को निष्क्रिय करें"

#: ../authconfig.py:150
msgid "default kerberos KDC"
msgstr "मूलभूत करबरोस KDC"

#: ../authconfig.py:152
msgid "default kerberos admin server"
msgstr "मूलभूत करबरोस प्रशासक सर्वर"

#: ../authconfig.py:153 ../authconfig.py:183
msgid "<realm>"
msgstr "<realm>"

#: ../authconfig.py:154
msgid "default kerberos realm"
msgstr "मूलभूत करबरोस रेल्म"

#: ../authconfig.py:156
msgid "enable use of DNS to find kerberos KDCs"
msgstr "करबरोस KDC को पाने में DNS के प्रयोग को सक्रिय करें"

#: ../authconfig.py:158
msgid "disable use of DNS to find kerberos KDCs"
msgstr "करबरोस KDC को पाने में DNS के प्रयोग को निष्क्रिय करें"

#: ../authconfig.py:160
msgid "enable use of DNS to find kerberos realms"
msgstr "करबरोस रियलम्स को पाने में DNS के प्रयोग को सक्रिय करें"

#: ../authconfig.py:162
msgid "disable use of DNS to find kerberos realms"
msgstr "करबरोस रियलम्स को पाने में DNS के प्रयोग को निष्क्रिय करें"

#: ../authconfig.py:165
msgid "enable SMB authentication by default"
msgstr "SMB सत्यापन को मूलभूल रूप से सक्रिय करें"

#: ../authconfig.py:167
msgid "disable SMB authentication by default"
msgstr "SMB सत्यापन को मूलभूल रूप से निष्क्रिय करें"

#: ../authconfig.py:168
msgid "<servers>"
msgstr "<servers>"

#: ../authconfig.py:169
msgid "names of servers to authenticate against"
msgstr "इसके बरक्स सर्वरों के नामों को सत्यापित करें"

#: ../authconfig.py:170
msgid "<workgroup>"
msgstr "<workgroup>"

#: ../authconfig.py:171
msgid "workgroup authentication servers are in"
msgstr "कार्यसमूह सत्यापन सर्वर इसमें हैं"

#: ../authconfig.py:174
msgid "enable winbind for user information by default"
msgstr "मूलभूत रूप से उपयोक्ता जानकारी के लिए विंडविंड को सक्रिय करें"

#: ../authconfig.py:176
msgid "disable winbind for user information by default"
msgstr "मूलभूत रूप से उपयोक्ता जानकारी के लिए विंडबिंड को निष्क्रिय करें"

#: ../authconfig.py:178
msgid "enable winbind for authentication by default"
msgstr "मूलभूत रूप से सत्यापन के लिए winbind को सक्रिय करें"

#: ../authconfig.py:180
msgid "disable winbind for authentication by default"
msgstr "मूलभूत रूप से सत्यापन के लिए winbind को निष्क्रिय करें"

#: ../authconfig.py:182
msgid "security mode to use for samba and winbind"
msgstr "सांबा और विंडबिंड के प्रयोग करने के लिए सुरक्षा मोड"

#: ../authconfig.py:184
msgid "default realm for samba and winbind when security=ads"
msgstr "सांबा और विंडबिंड के लिए मूलभूत रियल्म जब security=ads"

#: ../authconfig.py:185 ../authconfig.py:187
msgid "<lowest-highest>"
msgstr "<lowest-highest>"

#: ../authconfig.py:186
msgid "uid range winbind will assign to domain or ads users"
msgstr "uid रेंज विंडबिंड डोमेन या ads users को सौंपेगा"

#: ../authconfig.py:188
msgid "gid range winbind will assign to domain or ads users"
msgstr "gid रेंज विंडबिंड डोमेन या एड्स यूजर को सौंपेगा"

#: ../authconfig.py:190
msgid ""
"the character which will be used to separate the domain and user part of "
"winbind-created user names if winbindusedefaultdomain is not enabled"
msgstr ""
"अक्षर जो डोमेन और विंडबिंड निर्मित उपभोक्ता नाम को अलग करने के लिए प्रयुक्त होगा अगर "
"winbindusedefaultdomain सक्रिय नहीं है"

#: ../authconfig.py:192
msgid "the directory which winbind-created users will have as home directories"
msgstr "डायरेक्टरी जिसे विंडबिंड-निर्मित उपभोक्ता गृह डायरेक्टरी के रूप में रखेगा"

#: ../authconfig.py:194
msgid "the group which winbind-created users will have as their primary group"
msgstr "समूह जिसे विंडबिंड-निर्मित उपभोक्ता प्राथमिक समूह के रूप में रखेगा"

#: ../authconfig.py:196
msgid "the shell which winbind-created users will have as their login shell"
msgstr "शैल जिसे विंडबिंड-निर्मित उपभोक्ता अपने लॉगिन शैल के रूप में रखेगा"

#: ../authconfig.py:198
msgid ""
"configures winbind to assume that users with no domain in their user names "
"are domain users"
msgstr ""
"यह मानते हुए विंडबिंड सक्रिय करें कि अपने उपभोक्ता नाम में बिना डोमेन के उपभोक्ता डोमेन "
"उपभोक्ता हैं"

#: ../authconfig.py:200
msgid ""
"configures winbind to assume that users with no domain in their user names "
"are not domain users"
msgstr ""
"यह मानते हुए विंडबिंड सक्रिय करें कि अपने उपभोक्ता नाम में बिना डोमेन के उपभोक्ता डोमेन "
"उपभोक्ता नहीं हैं"

#: ../authconfig.py:202
msgid "configures winbind to allow offline login"
msgstr "ऑफलाइन लॉगिन स्वीकारने के लिए winbind विन्यस्त करता है"

#: ../authconfig.py:204
msgid "configures winbind to prevent offline login"
msgstr "ऑफलाइन लॉगिन रोकने के लिए winbind विन्यस्त करता है"

#: ../authconfig.py:206
msgid "join the winbind domain or ads realm now as this administrator"
msgstr "अब इस प्रशासक के रूप में विंडबिंड डोमेन या ads रियल्म से जुड़ें"

#: ../authconfig.py:209
msgid "enable wins for hostname resolution"
msgstr "मेजबाननाम समाधान के लिए विंस सक्रिय करें"

#: ../authconfig.py:211
msgid "disable wins for hostname resolution"
msgstr "मेजबाननाम समाधान के लिए विंस निष्क्रिय करें"

#: ../authconfig.py:214
msgid "prefer dns over wins or nis for hostname resolution"
msgstr "मेजबाननाम समाधान के लिए विंस या निस के बजाए dns पसंद करें"

#: ../authconfig.py:216
msgid "do not prefer dns over wins or nis for hostname resolution"
msgstr "मेजबाननाम समाधान के लिए विंस या निस के बजाए dns पसंद मत करें"

#: ../authconfig.py:219
msgid "enable hesiod for user information by default"
msgstr "मूलभूत रूप से उपयोक्ता जानकारी के लिए हेसियड को सक्रिय करें"

#: ../authconfig.py:221
msgid "disable hesiod for user information by default"
msgstr "मूलभूत रूप से उपयोक्ता जानकारी के लिए हेसियड को निष्क्रिय करें"

#: ../authconfig.py:223
msgid "default hesiod LHS"
msgstr "मूलभूत हेसियड LHS"

#: ../authconfig.py:225
msgid "default hesiod RHS"
msgstr "मूलभूत हेसियड RHS"

#: ../authconfig.py:228
msgid "enable caching of user information by default"
msgstr "मूलभूत रूप से उपयोक्ता जानकारी के केश को सक्रिय करें"

#: ../authconfig.py:230
msgid "disable caching of user information by default"
msgstr "मूलभूत रूप से उपयोक्ता जानकारी के केश को निष्क्रिय करें"

#: ../authconfig.py:233
msgid "local authorization is sufficient for local users"
msgstr "स्थानीय दावेदारी स्थानीय उपयोक्ताओं के लिये पर्याप्त है"

#: ../authconfig.py:235
msgid "authorize local users also through remote service"
msgstr "दूरस्थ सेवा के द्वारा भी स्थानीय उपयोक्ताओं को प्राधिकृत करें"

#: ../authconfig.py:238
msgid "check access.conf during account authorization"
msgstr "सत्यापन के दौरान access.conf जांचें"

#: ../authconfig.py:240
msgid "do not check access.conf during account authorization"
msgstr "खाता सत्यापन के दौरान access.conf मत जांचें"

#: ../authconfig.py:243
msgid "authenticate system accounts by network services"
msgstr "संजाल सेवा के द्वारा सिस्टम प्रमाणित करता है"

#: ../authconfig.py:245
msgid "authenticate system accounts by local files only"
msgstr "सिर्फ स्थानीय फाइल के द्वारा सिस्टम खाता प्रमाणित करता है"

#: ../authconfig.py:248
msgid "create home directories for users on their first login"
msgstr "उनके पहले लॉगिन पर उपयोक्ता के लिए घर निर्देशिका"

#: ../authconfig.py:250
msgid "do not create home directories for users on their first login"
msgstr "उनके पहले लॉगिन पर उपयोक्ताओं के लिए घर निर्देशिका नहीं बनाएं"

#: ../authconfig.py:253
msgid "do not start/stop portmap, ypbind, and nscd"
msgstr "portmap, ypbind, और nscd को आरंभ/अंत न करें "

#: ../authconfig.py:256
msgid "do not update the configuration files, only print new settings"
msgstr "विन्यास फाइल अद्यतन मत करें, सिर्फ नयी जमावट को छापें"

#: ../authconfig.py:260
msgid "display Back instead of Cancel in the main dialog of the TUI"
msgstr "TUI के मुख्य संवाद में रद्द के बजाय पीछे को दिखायें"

#: ../authconfig.py:262
msgid "do not display the deprecated text user interface"
msgstr "पदावनत पाठ उपयोक्ता अंतरफलक को न दिखायें"

#: ../authconfig.py:265
msgid "opposite of --test, update configuration files with changed settings"
msgstr "--test का विलोम, बदले जमावट के साथ विन्यास फाइल अद्यतन करें"

#: ../authconfig.py:268
msgid "update all configuration files"
msgstr "सारे विन्यास फाइल अद्यतन करें"

#: ../authconfig.py:271
msgid "probe network for defaults and print them"
msgstr "मूलभूतों के लिए संजाल खोजें और उन्हें छापें"

#: ../authconfig.py:273 ../authconfig.py:276
msgid "<name>"
msgstr "<name>"

#: ../authconfig.py:274
msgid "save a backup of all configuration files"
msgstr "सभी विन्यास फाइल के बैकअफ को सहेजता है"

#: ../authconfig.py:277
msgid "restore the backup of configuration files"
msgstr "विन्यास फाइल के बैकअप को पुनर्भंडारित करता है"

#: ../authconfig.py:280
msgid ""
"restore the backup of configuration files saved before the previous "
"configuration change"
msgstr "पिछले विन्यास बदलाव के पहले विन्यास फाइल के बैकअप को पुनर्भंडारित करता है"

#: ../authconfig.py:285
msgid "unexpected argument"
msgstr "अप्रत्याशित तर्क"

#: ../authconfig.py:401
msgid "Bad smart card removal action specified."
msgstr "खराब स्मार्टकार्ड विस्थापन क्रिया निर्दिष्ट."

#: ../authconfig.py:410
msgid "Unknown password hashing algorithm specified, using sha256."
msgstr "अज्ञात शब्दकूट हैशिंग अलगोरिथम निर्दिष्ट, sha256 का प्रयोग कर रहा है."

#: ../authconfig.py:443
msgid "can only be run as root"
msgstr "सिर्फ रूट होकर ही चला सकते हैं"

#: ../authconfig.py:459
msgid "dialog was cancelled"
msgstr "डाइलॉग को रद्द किया गया था"

#: ../authconfig.py:483
#, python-format
msgid ""
"The %s file was not found, but it is required for %s support to work "
"properly.\n"
"Install the %s package, which provides this file."
msgstr ""
"%s फाइल मिली नहीं, मगर %s समर्थन के लिए यह आवश्यक है.\n"
"इस फाइल को देने वाले %s संकुल का अधिष्ठापन करें."

#: ../authconfig.py:485 ../authconfig.py:831
msgid "Warning"
msgstr "चेतावनी"

#: ../authconfig.py:485 ../authconfig.py:702 ../authconfig.py:709
#: ../authconfig.py:715 ../authconfig.py:724 ../authconfig.py:732
#: ../authconfig.py:773 ../authconfig.py:831
msgid "Ok"
msgstr "ठीक"

#: ../authconfig.py:489
msgid "caching"
msgstr "कैशिंग"

#: ../authconfig.py:490
msgid "Fingerprint reader"
msgstr "फिंगरप्रिंट पाठक"

#: ../authconfig.py:491
msgid "Kerberos"
msgstr "करबरोस"

#: ../authconfig.py:492
msgid "LDAP authentication"
msgstr "LDAP सत्यापन"

#: ../authconfig.py:493
msgid "LDAP"
msgstr "LDAP"

#: ../authconfig.py:494
msgid "NIS"
msgstr "NIS"

#: ../authconfig.py:495
msgid "shadow password"
msgstr "छाया शब्दकूट"

#: ../authconfig.py:496 ../authconfig.py:498
msgid "Winbind"
msgstr "विंडवाइंड"

#: ../authconfig.py:497
msgid "Winbind authentication"
msgstr "विंडवाइंड सत्यापन"

#: ../authconfig.py:503
msgid "User Information"
