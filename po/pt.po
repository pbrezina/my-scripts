#: ../authconfig.c:1266
#, c-format
msgid ""
"Usage: %s [options]\n"
"\n"
"     --nostart            do not start/stop yp\n"
"     --enablenis          enable nis by default\n"
"     --nisdomain <domain> default NIS domain\n"
"     --nisserver <server> default NIS server\n"
"     --useshadow          use shadow passwords\n"
"     --enablemd5          enable MD5 passwords\n"
"     --kickstart          don't display user interface\n"
"     --help               show this screen\n"
msgstr ""
"Utilização: %s [opções]\n"
"\n"
"     --nostart              não arrancar/parar yp\n"
"     --enablenis            activar NIS por omissão\n"
"     --nisdomain <domínio>  domínio NIS por omissão\n"
"     --nisserver <servidor> servidor NIS por omissão\n"
"     --useshadow            utilizar senhas \"shadow\"\n"
"     --enablemd5            activar senhas MD5\n"
"     --kickstart            não mostrar inteface\n"
"     --help                 mostrar esta ajuda\n"

