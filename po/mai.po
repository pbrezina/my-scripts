#: ../authconfig.py:80
#, python-format
msgid "usage: %s [options]"
msgstr "प्रयोग: %s [विकल्प]"

#: ../authconfig.py:87
msgid "enable shadowed passwords by default"
msgstr "पूर्वनिर्धारित रूपेँ छायांकित गुड़किल्ली केँ सक्रिय करू"

#: ../authconfig.py:89
msgid "disable shadowed passwords by default"
msgstr "पूर्वनिर्धारित रूपेँ छायांकित गुड़किल्ली केँ निष्क्रिय करू"

#: ../authconfig.py:91
msgid "enable MD5 passwords by default"
msgstr "पूर्वनिर्धारित रूपेँ MD5 गुड़किल्ली केँ सक्रिय करू"

#: ../authconfig.py:93
msgid "disable MD5 passwords by default"
msgstr "पूर्वनिर्धारित रूपेँ MD5 गुड़किल्ली केँ निष्क्रिय करू"

#: ../authconfig.py:96
